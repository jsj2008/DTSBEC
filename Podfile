# CocoaPods master repo

source 'https://github.com/CocoaPods/Specs.git'
source 'https://dengtacj@bitbucket.org/dengtacj/dtspecs.git'

# ignore all warnings from all pods
inhibit_all_warnings!
use_frameworks!

platform :ios, '7.0'


def shared_pods

pod 'DTSThoth’,'~> 0.3.0'

end

target 'DTSBEC’ do
    shared_pods
end


